<mvc:View
	controllerName="sap.dm.controller.Home"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:h="http://www.w3.org/1999/xhtml">
	<Page
		id="page"
		title="Appointment"
		showFooter="{device>/isNoPhone}">
		<headerContent>
			<Button
				icon="sap-icon://delete"
				tooltip=""
				press="">
			</Button>
		</headerContent>
		<subHeader>
			<Toolbar id="searchBar">
				<SearchField
					id="searchField"
					showRefreshButton="{device>/isNoTouch}"
					placeholder=""
					search="handleSearch"
					tooltip="{i18n>HOME_SEARCH_TOOLTIP}"
					width="100%">
				</SearchField>
			</Toolbar>
		</subHeader>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
		<content>
			<Button text="Home"></Button>
		</content>
	</Page>
</mvc:View>