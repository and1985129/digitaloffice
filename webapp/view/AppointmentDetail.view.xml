<mvc:View
	controllerName="sap.dm.controller.AppointmentDetail"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="Appointment Navigation"
		navButtonPress="handleNavBack"
		showNavButton="{device>/isPhone}">
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
		<content>
			<VBox>
				<Label text="{/content}"></Label>
				<Image
					densityAware="false"
					src="img/bigmap.png"></Image>
			</VBox>
		</content>
	</Page>
</mvc:View>